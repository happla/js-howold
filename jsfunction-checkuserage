	if (isNaN(ika) || ika < 0) {
		console.log("Syötetty ikä:", ika)
		console.log("Virheellinen syöte!")
	} else if (ika<18) {
		console.log("Syötetty ikä:", ika)
		console.log("Käyttäjä ei ole vielä täysi-ikäinen.")
	} else {
		console.log("Syötetty ikä:", ika)
		console.log("Käyttäjä on täysi-ikäinen.")
	}
