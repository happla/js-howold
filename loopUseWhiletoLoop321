
// Put your code here
function loop() {
	while (value>0){
		console.log(value);
		value--;
	}
}

var value = 3;

var greaterThanZero = function (n) {
		return n > 0;
}

var reduceOne = function (n) {
	return n - 1;
}

var printValue = function (n) {
	console.log(n);
}

loop(value, greaterThanZero, reduceOne, printValue);
